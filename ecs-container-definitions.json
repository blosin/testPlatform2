{
  "containerDefinitions": [
    {
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 3087
        }
      ],
      "cpu": 0,
      "memory": 256,
      "image": "382381053403.dkr.ecr.us-east-2.amazonaws.com/smartfran/platform-service:ps-2.2.1",
      "essential": true,
      "name": "platform-service-staging-container",
      "entryPoint": [
        "npm",
        "run"
      ],
      "command": [
        "serve"
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "staging"
        }
      ]
    }
  ],
  "tags": [
    {
      "key": "Customer",
      "value": "Smartfran"
    },
    {
      "key": "Environment",
      "value": "staging"
    },
    {
      "key": "Project",
      "value": "platform-service"
    }
  ],
  "family": "platform-service-staging-task",
  "taskRoleArn": "arn:aws:iam::382381053403:role/smartfran-task-role"
}