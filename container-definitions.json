{
  "containerDefinitions": [
    {
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 3085
        }
      ],
      "cpu": 0,
      "memory": 256,
      "image": "${DOCKER_REPOSITORY}/${PROJ_REPO}:${IMG_TAG}",
      "essential": true,
      "name": "${SERVICE_NAME}-${BRANCH_NAME}-container",
      "entryPoint": [
        "npm",
        "run"
      ],
      "command": [
        "serve"
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "${BRANCH_NAME}"
        }
      ]
    }
  ],
  "tags": [
    {
      "key": "Customer",
      "value": "Smartfran"
    },
    {
      "key": "Environment",
      "value": "${BRANCH_NAME}"
    },
    {
      "key": "Project",
      "value": "Concentrador"
    }
  ],
  "family": "${SERVICE_NAME}-${BRANCH_NAME}-task",
  "taskRoleArn":"arn:aws:iam::382381053403:role/smartfran-task-role"

}