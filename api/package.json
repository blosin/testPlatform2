{
  "name": "platforms-service",
  "description": "Platform service for SmartfranPedidos Project",
  "main": "src/index.js",
  "author": "Ross",
  "scripts": {
    "build": "npm-run-all clean transpile",
    "serve": "node dist/index.js",
    "serve:tst": "NODE_ENV=testing npm run serve",
    "serve:stg": "NODE_ENV=staging npm run serve",
    "serve:prd": "NODE_ENV=production npm run serve",
    "start": "AWS_PROFILE=smartfran nodemon --inspect=127.0.0.1:9230  src/index.js --watch src --exec babel-node",
    "start:win": "nodemon --inspect=127.0.0.1:9230  src/index.js --watch src --exec babel-node",
    "start:dev": "NODE_ENV=development npm run start",
    "start:tst": "NODE_ENV=testing npm run start",
    "start:stg": "NODE_ENV=staging npm run start",
    "test": "./node_modules/mocha/bin/mocha test/unit --require @babel/register --recursive --exit",
    "test:coverage": "nyc --reporter=lcov ./node_modules/mocha/bin/mocha  test/unit --require @babel/register -x **/sdk/pedidosYa/**,**/config/**,**/assets/**,**/routes/** --recursive --exit",
    "test:integration": "./node_modules/newman/bin/newman.js run --bail --verbose --environment 'test/integration/environment.json' 'test/integration/collection.json' --delay-request 2000 --export-environment 'test/integration/environment.json'",
    "seed": "node ./seeders/helpers/data_generator.js && DEBUG=mongo-seeding node seeders/index.js",
    "seed-start": "npm-run-all seed serve",
    "clean": "rimraf dist",
    "transpile": "babel src --ignore node_modules,src/platforms/sdk/pedidosYa/node_modules --copy-files  --out-dir dist"
  },
  "dependencies": {
    "@meanie/mongoose-to-json": "^2.4.0",
    "aws-sdk": "^2.738.0",
    "axios": "^0.19.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "core-js": "^3.2.1",
    "cors": "^2.8.5",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "mongoose": "^5.4.20",
    "mongoose-unique-validator": "^2.0.2",
    "morgan": "^1.9.1",
    "node-cron": "^2.0.3",
    "regenerator-runtime": "^0.13.3",
    "sqs-consumer": "^5.4.0",
    "uuid": "8.0.0",
    "winston": "3.2.1",
    "winston-mongodb": "5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/compat-data": "^7.8.0",
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.8.6",
    "@babel/runtime": "^7.6.2",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "colonize": "^1.0.5",
    "eslint": "^6.2.2",
    "mocha": "^6.2.2",
    "mongo-seeding": "^3.4.0-alpha.1",
    "newman": "^5.0.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rewire": "^4.0.1",
    "sinon": "^7.5.0"
  },
  "resolutions": {
    "@babel/preset-env": "7.5.5"
  }
}
