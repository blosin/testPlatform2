{
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/concentrador-pedidos-production-task",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "portMappings": [
        {
          "hostPort": 3085,
          "protocol": "tcp",
          "containerPort": 3085
        }
      ],
      "image": "265556570017.dkr.ecr.us-west-2.amazonaws.com/smartfran/api:${IMG_TAG}",
      "essential": true,
      "name": "concentrador-api",
      "entryPoint": [
        "npm",
        "run"
      ],
      "command": [
        "serve"
      ],
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        }
      ]
    }
  ],
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::382381053403:role/ecsTaskExecutionRole",
  "tags": [
    {
      "key": "Environment",
      "value": "Production"
    },
    {
      "key": "Project",
      "value": "Concentrador"
    }
  ],
  "family": "concentrador-pedidos-production-task"
}